<div id="body_content">

    <table cellpadding="5" cellspacing="0" width="95%">
      <thead>
      <tr>
        <th>ID</th>
        <th>登录名</th>
        <th>真实姓名</th>
        <th>角色</th>
        <th>最后登录时间</th>
        <th>登录次数</th>
        <th>状态</th>
      </tr>
      </thead>
    <% i=0 %>
    <% @admins.each do |admin| %>
      <tr <%= (i+=1)%2==0?"style=background:#EEF4FA":"" %>>
        <td><%= admin.id %></td>
        <td><%= admin.login_name %></td>
        <td><%= admin.username %></td>
        <td><%= admin.role %></td>
        <td><%= admin.last_login_time %></td>
        <td><%= admin.login_count %></td>
        <td><%= admin.state==1?"正常":"禁用" %></td>
      </tr>
    <% end %>
    </table>
  
<br><br><br><br><br><br><br><br>
<br><br><br><br><br><br><br><br>
</div>
